{
  "rewrites": [
    {
      "source": "/images/(.*)",
      "destination": "/images/$1"
    },
    { 
      "source": "/(.*)", "destination": "/index.html" 
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
